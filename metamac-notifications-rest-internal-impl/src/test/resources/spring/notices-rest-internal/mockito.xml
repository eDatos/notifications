<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:p="http://www.springframework.org/schema/p"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
    
    <bean id="noticeService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="org.siemac.metamac.notices.core.notice.serviceapi.NoticesService" />
    </bean>
    
    <bean id="externalItemRepository" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="org.siemac.metamac.notices.core.common.domain.ExternalItemRepository" />
    </bean>
    
    <bean id="internationalStringRepository" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="org.siemac.metamac.notices.core.common.domain.InternationalStringRepository" />
    </bean>
    
</beans>
